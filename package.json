{
  "name": "freerange-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "freerange-core",
    "freerange-server"
  ],
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "tinybuild",
    "backend": "node --max-http-header-size=16000 example/backend/main.js",
    "which": "which tinybuild"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "cors": "^2.8.5",
    "tinybuild": "0.3.129",
    "typescript": "^4.7.3"
  }
}
